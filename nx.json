{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "nxCloudAccessToken": "Y2MxNmI0YWItNDdhYS00ZWUxLTliZjMtODkzYjZkYTE4YjgxfHJlYWQtd3JpdGU=",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "update-rule-docs": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "update-rule-lists": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "e2e": {
      "cache": true
    },
    "integration-test": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "check-rule-docs": {
      "cache": true
    },
    "check-rule-lists": {
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals", "projectSpecificFiles"],
    "sharedGlobals": [
      "{workspaceRoot}/.github/workflows/ci.yml",
      {
        "runtime": "node -e 'console.log(`${process.platform}-${process.arch}`)'"
      },
      {
        "runtime": "node --version"
      },
      {
        "runtime": "npm --version"
      },
      {
        "runtime": "yarn --version"
      }
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s"
    ],
    "projectSpecificFiles": []
  }
}
