{
  "npmScope": "angular-eslint",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*",
    ".github/workflows/ci.yml": "*"
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "Y2MxNmI0YWItNDdhYS00ZWUxLTliZjMtODkzYjZkYTE4YjgxfHJlYWQtd3JpdGU=",
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "e2e",
          "integration-tests",
          "check-configs",
          "typecheck"
        ],
        "runtimeCacheInputs": ["node --version"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "builder": {
      "implicitDependencies": []
    },
    "eslint-plugin": {
      "implicitDependencies": []
    },
    "eslint-plugin-template": {
      "implicitDependencies": []
    },
    "integration-tests": {
      "implicitDependencies": [
        "builder",
        "eslint-plugin",
        "eslint-plugin-template",
        "schematics",
        "template-parser",
        "utils"
      ]
    },
    "schematics": {
      "implicitDependencies": []
    },
    "template-parser": {
      "implicitDependencies": []
    },
    "utils": {
      "implicitDependencies": []
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
