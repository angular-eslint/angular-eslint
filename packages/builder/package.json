{
  "name": "@angular-eslint/builder",
  "version": "0.0.1-alpha.24",
  "description": "Angular CLI builder for ESLint",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json && terser ./dist/index.js --output=./dist/index.js",
    "postbuild": "cp src/schema.json dist/",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-eslint/angular-eslint.git",
    "directory": "packages/builder"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "builders.json"
  ],
  "builders": "./builders.json",
  "peerDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "eslint": "*",
    "typescript": "*"
  },
  "gitHead": "a1fa84453b178ad182f25c33d9a147e67881ef54"
}
