{
  "name": "template-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "type": "library",
  "targets": {
    "test": {
      "options": {
        "args": "run"
      },
      "configurations": {
        "ci": {
          "args": ["run", "--coverage"]
        }
      }
    },
    "bench": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vitest bench --run",
        "cwd": "{projectRoot}"
      }
    }
  }
}
